
public class FloatExample {
	public static void main(String[] args) {
		float f1 = 7.12345F;
		double d1 = 7.12345;
		System.out.println(f1);
		System.out.println(d1);
		
		
		float f2 = 1.234567891234F;
		double d2 = 1.234567891234561234;
		
		System.out.println(f2);
		System.out.println(d2);
		
		
	}
}
